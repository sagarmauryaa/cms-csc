{
  "kind": "collectionType",
  "collectionName": "acdemic_courses",
  "info": {
    "singularName": "acdemic-course",
    "pluralName": "acdemic-courses",
    "displayName": "Acdemic Course"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string",
      "required": true
    },
    "Slug": {
      "type": "uid",
      "targetField": "Name",
      "required": true
    },
    "acdemic": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::acdemic.acdemic",
      "private": true
    },
    "Seo": {
      "type": "component",
      "component": "common.seo",
      "repeatable": false
    },
    "content": {
      "type": "dynamiczone",
      "components": [
        "member.faculty",
        "common.docs"
      ]
    }
  }
}
